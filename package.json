{
  "name": "7-docs",
  "version": "0.0.5",
  "description": "Build a knowledge base and ask it questions through ChatGPT",
  "bin": {
    "7d": "./dist/cli.js"
  },
  "type": "module",
  "homepage": "https://github.com/webpro/7-docs",
  "repository": "github:webpro/7-docs",
  "bugs": "https://github.com/webpro/7-docs/issues",
  "license": "ISC",
  "author": {
    "name": "Lars Kappert",
    "email": "lars@webpro.nl"
  },
  "scripts": {
    "format": "remark README.md -o",
    "prebuild": "node -e \"require('fs').rmSync('dist', { force: true, recursive: true })\"",
    "watch": "tsc --watch",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "docs",
    "documentation",
    "kb",
    "markdown",
    "vector",
    "database",
    "embedding",
    "chatgpt",
    "openai",
    "pinecone",
    "supabase"
  ],
  "dependencies": {
    "@octokit/rest": "^19.0.7",
    "@pinecone-database/pinecone": "^0.0.12",
    "@supabase/supabase-js": "^2.15.0",
    "dotenv": "^16.0.3",
    "fast-glob": "^3.2.12",
    "gpt-3-encoder": "^1.1.4",
    "micromatch": "^4.0.5",
    "openai": "^3.2.1",
    "ora": "^6.3.0",
    "remark": "^14.0.2",
    "remark-gfm": "^3.0.1",
    "strip-markdown": "^5.0.0"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.2",
    "@types/node": "^18.15.11",
    "remark-cli": "^11.0.0",
    "remark-preset-webpro": "^0.0.2",
    "tsx": "^3.12.6",
    "typescript": "^5.0.3"
  },
  "remarkConfig": {
    "plugins": [
      "preset-webpro"
    ]
  }
}
